{
	"name": "lightingo",
	"version": "0.0.1",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "yarn lint:ts && yarn lint:style && yarn lint:prettier",
		"lint-fix": "yarn lint:ts-fix && yarn lint:style-fix && yarn lint:prettier-fix",
		"lint:ts": "ng lint",
		"lint:ts-fix": "ng lint --fix",
		"lint:style": "stylelint \"**/*.scss\"",
		"lint:style-fix": "yarn lint:style --fix",
		"lint:prettier": "prettier --list-different \"**/*{.js,.ts,.json,.css,.scss,.html}\"",
		"lint:prettier-fix": "yarn lint:prettier --write",
		"e2e": "ng e2e",
		"postinstall": "husky install && ngcc",
		"generate-icons": "svg-generator"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~11.2.1",
		"@angular/common": "~11.2.1",
		"@angular/compiler": "~11.2.1",
		"@angular/core": "~11.2.1",
		"@angular/forms": "~11.2.1",
		"@angular/platform-browser": "~11.2.1",
		"@angular/platform-browser-dynamic": "~11.2.1",
		"@angular/router": "~11.2.1",
		"@ngneat/svg-icon": "2.3.0",
		"@ngrx/effects": "^11.0.1",
		"@ngrx/router-store": "^11.0.1",
		"@ngrx/store": "^11.0.1",
		"@ngrx/store-devtools": "^11.0.1",
		"primeflex": "^2.0.0",
		"primeicons": "^4.1.0",
		"primeng": "^11.2.3",
		"rxjs": "~6.6.0",
		"tslib": "^2.0.0",
		"zone.js": "~0.11.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1102.1",
		"@angular/cli": "~11.2.1",
		"@angular/compiler-cli": "~11.2.1",
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@ngneat/spectator": "^7.0.0",
		"@ngneat/svg-generator": "^1.0.0",
		"@types/jasmine": "~3.6.0",
		"@types/node": "^12.11.1",
		"codelyzer": "^6.0.0",
		"husky": "^5.0.9",
		"jasmine-core": "~3.6.0",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "~6.1.0",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage": "~2.0.3",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"lint-staged": "^10.5.4",
		"ng-mocks": "^11.9.1",
		"prettier": "^2.2.1",
		"protractor": "~7.0.0",
		"stylelint": "^13.10.0",
		"stylelint-config-sass-guidelines": "^8.0.0",
		"ts-node": "~8.3.0",
		"tslint": "~6.1.0",
		"typescript": "~4.1.2"
	},
	"lint-staged": {
		"*.ts": [
			"tslint --project ."
		],
		"*.scss": [
			"stylelint --syntax=scss"
		],
		"*{.ts,.js,.json,.css,.scss,.html}": [
			"prettier --config .prettierrc.json --ignore-path .prettierignore --list-different \"**/*{.js,.ts,.json,.css,.scss,.html}\""
		]
	},
	"svgGenerator": {
		"outputPath": "./src/assets/svg",
		"prefix": "app",
		"srcPath": "./src/assets/img",
		"outputDirectory": "./src/assets/svg",
		"svgoConfig": {
			"plugins": [
				{
					"removeDimensions": true,
					"cleanupAttrs": true
				}
			]
		}
	}
}
